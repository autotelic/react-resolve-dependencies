{
  "name": "react-resolve-dependencies",
  "version": "0.1.1",
  "description": "Resolve dependencies before rendering a React component",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "rimraf lib && babel src --out-dir lib",
    "build:watch": "watch 'npm run build' ./src/",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --recursive ./test --require ./test/setup.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "keywords": [],
  "author": "Edward Stone &lt;edwardjstone@yahoo.com&gt;",
  "repository": "https://github.com/eadmundo/react-resolve-dependencies",
  "bugs": {
    "url": "https://github.com/eadmundo/react-resolve-dependencies/issues"
  },
  "homepage": "https://github.com/eadmundo/react-resolve-dependencies",
  "license": "MIT",
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.5.1",
    "eslint": "~2.2.0",
    "jsdom": "^9.8.3",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.3.2",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.6",
    "watch": "^0.17.1"
  },
  "npmName": "react-resolve-dependencies",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ]
}
