{
  "name": "react-resolve-dependencies",
  "version": "0.2.0",
  "description": "Resolve dependencies before rendering a React component",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "rimraf lib && babel src --out-dir lib",
    "build:watch": "watch 'npm run build' ./src/",
    "prepublish": "npm run build",
    "test": "mocha --require @babel/register --recursive ./test --require ./test/setup.js"
  },
  "keywords": [],
  "author": "Edward Stone &lt;edwardjstone@yahoo.com&gt;",
  "repository": "https://github.com/eadmundo/react-resolve-dependencies",
  "bugs": {
    "url": "https://github.com/eadmundo/react-resolve-dependencies/issues"
  },
  "homepage": "https://github.com/eadmundo/react-resolve-dependencies",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "hoist-non-react-statics": "^3.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jsdom": "^15.1.1",
    "mocha": "^6.2.0",
    "react-addons-test-utils": "^15.6.2",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "watch": "^1.0.2"
  },
  "npmName": "react-resolve-dependencies",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ]
}
